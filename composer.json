{
    "name": "lucatume/route-pages",
    "description": "A WP Router extension to manage route created pages.",
    "authors": [
        {
            "name": "theAverageDev (Luca Tumedei)",
            "email": "luca@theaveragedev.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "badcow/lorem-ipsum": "dev-master",
        "lucatume/tdd-helpers": "2.*    ",
        "lucatume/wp-utils": "2.*",
        "xrstf/composer-php52": "1.*"
    },
    "scripts": {
        "post-install-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-update-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-autoload-dump": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ]
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "require-dev": {
        "codeception/codeception": "2.*",
        "lucatume/wp-browser": "1.*"
    }
}
